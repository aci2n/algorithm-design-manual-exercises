#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <stdbool.h>
#include <assert.h>
#include <time.h>

typedef int ms_compare(void const*, void const*);

void mergesort_merge(size_t const start, size_t const end, void* arr[restrict static end - start]) {
}

void mergesort_inner(size_t const start, size_t const end, void* arr[restrict static end - start]) {
  if (start == end) {
    return arr;
  }
  size_t const mid = (start + end) / 2;
  mergesort_inner(start, mid);
  mergesort_inner(mid + 1, end);
  mergesort_merge(start, end, arr);
}

void mergesort(size_t const n, void* arr[restrict static n]) {
  mergesort_inner(0, n - 1, arr);
}

int main(int const argc, char const*const argv[static argc + 1]) {
  srand(time(0));
  return EXIT_SUCCESS;
}
